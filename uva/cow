#include<stdio.h>
int main()
{
    int x1,y1,x2,y2,x,y,t,n,m;
    scanf("%d",&n);
    while(n--)
	{
		scanf("%d%d%d%d",&x1,&y1,&x2,&y2);
		scanf("%d",&m);
		t=0;
		while(++t<=m)
		{
			scanf("%d%d",&x,&y);
			if((x>x1 && x< x2) &&(y>y1 && y< y2))
		printf("YES\n");
		else
		printf("NO\n");
	    }
	}
    return 0;
}